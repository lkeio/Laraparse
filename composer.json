{
      "name": "lkeio/laraparse",
      "description": "Laravel 5.5 package, this package is a wrapper around erusev/parsedown for Laravel",
      "keywords": ["markdown", "parsedown", "laravel"],
      "type": "package",
      "license": "MIT",
      "authors": [
          {
              "name": "lkeio",
              "email": "github@lkamal.com"
          }
      ],
      "prefer-stable": true,
      "minimum-stability": "stable",
      "require": {
          "erusev/parsedown": "^1.6"
      },
      "require-dev": {
            "phpunit/phpunit": "^7",
            "orchestra/testbench": "~3.6"
      },
      "autoload": {
      "psr-4": {
          "Lkeio\\Laraparse\\": "src/"
        },
      "files": [
          "src/Laraparse.php"
        ]
      },
      "autoload-dev": {
      "psr-4": {
          "Tests\\": "tests/"
        }
      },
      "extra": {
        "laravel": {
            "providers": [
                "Lkeio\\Laraparse\\LaraparseServiceProvider"
            ],
            "aliases": {
              "Laraparse": "Lkeio\\Laraparse\\Facades\\Laraparse"
            }
        }
  }

}
